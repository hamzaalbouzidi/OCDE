      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{B0DC49FE-B31E-4725-AD3B-5BB1D1A9DFE8}"
          SQLTask:SqlStatementSource="IF  EXISTS (SELECT * FROM sys.objects where name ='a')&#xA;drop TABLE  [dbo].a&#xA;BEGIN&#xA;&#xA;SELECT  [IPAddress_ID]&#xA;              , [IPAddress]&#xA;,(CONVERT(bigint, PARSENAME([IPAddress],1)) +  CONVERT(bigint, PARSENAME([IPAddress],2)) * 256 +&#xA;CONVERT(bigint, PARSENAME([IPAddress],3)) * 65536 +&#xA; &#xA; &#xA;        CONVERT(bigint, PARSENAME([IPAddress],4)) * 16777216) as IPNumber&#xA;INTO a&#xA;FROM [stg].[IPAddress_DM] &#xA;where ISNUMERIC(PARSENAME([IPAddress],1))=1 and ISNUMERIC(PARSENAME([IPAddress],2))=1 &#xA;and ISNUMERIC(PARSENAME([IPAddress],3))=1 and ISNUMERIC(PARSENAME([IPAddress],4))=1  &#xA;end&#xA;&#xA;IF  EXISTS (SELECT * FROM sys.objects where name ='b')&#xA;drop TABLE  [dbo].b&#xA;BEGIN&#xA;select [HBoundDec],[LBoundDec],CountryISO2 &#xA;into b&#xA;from [stg].[STG_Metadata_TMP_IP_Country_Range]&#xA;end&#xA;&#xA;IF  EXISTS (SELECT * FROM sys.objects where name ='d')&#xA;drop TABLE  [dbo].d&#xA;BEGIN&#xA;select Country_ISO2Code,Country_ID &#xA;into d &#xA;from [stg].[Country_DM]&#xA;end&#xA;&#xA;INSERT [stg].[STG_Metadata_IPCountry] (IPAddress_ID,Country_ID) &#xA;SELECT c.IPAddress_ID,d.Country_ID from &#xA;(SELECT  a.*,b.* &#xA;from dbo.a  &#xA;CROSS JOIN b where   IPNumber&lt;=b.[HBoundDec] AND IPNumber&gt;=b.[LBoundDec]) c &#xA;INNER JOIN d on c.CountryISO2=d.Country_ISO2Code&#xA;&#xA;drop table a &#xA;drop table b &#xA;drop table d&#xA; &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
